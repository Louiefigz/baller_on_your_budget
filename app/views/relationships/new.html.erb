


<%= form_for @relationship do |f| %>
<%= f.fields_for :updated_relationship do |u|%>
<%=select("description", "relationship_id", @relationships.collect {|r| [ r.description, r.id ] }, { :include_blank => true })%>
  <%= u.label :description %>
  <%= u.text_field :description %>
  <%= f.submit %>

<% end %>
